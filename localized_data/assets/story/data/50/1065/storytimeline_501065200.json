{
    "no_wrap": true,
    "title": "迎向出道戰",
    "text_block_list": [
        {
            "name": "大德太陽",
            "text": "嗚哇～～！\n來到賽場啦～～～～！"
        },
        {
            "name": "大德太陽",
            "text": "草地！空氣好新鮮！\n嗨起來了！！"
        },
        {
            "name": "大德太陽",
            "text": "已經可以跑了嗎！？",
            "choice_data_list": [
                "咦？"
            ]
        },
        {
            "name": "大德太陽",
            "text": "入閘啦～～！",
            "choice_data_list": [
                "！？"
            ]
        },
        {
            "name": "大德太陽",
            "text": "阿訓也一起～！",
            "choice_data_list": [
                "最……最棒的浪潮！"
            ]
        },
        {
            "name": "大德太陽",
            "text": "最棒的浪潮！？\n在哪！？",
            "choice_data_list": [
                "在正式比賽時跟上最棒的浪潮吧！"
            ]
        },
        {
            "name": "大德太陽",
            "text": "！！"
        },
        {
            "name": "大德太陽",
            "text": "現在先累積開心的情緒……",
            "choice_data_list": [
                "現在先一口氣累積開心的情緒……"
            ]
        },
        {
            "name": "大德太陽",
            "text": "嗯嗯！",
            "choice_data_list": [
                "然後在正式比賽時爆發！"
            ]
        },
        {
            "name": "大德太陽",
            "text": "我知道了──☆"
        },
        {
            "name": "大德太陽",
            "text": "那就試著保存嗨起來的心情吧！\n那我去換衣服啦～☆"
        },
        {
            "name": "目白善信",
            "text": "太陽！"
        },
        {
            "name": "大德太陽",
            "text": "善善～！\n喔耶～！"
        },
        {
            "name": "大德太陽",
            "text": "妳是來幫我加油的嗎！？\n咻，不愧是好麻吉！"
        },
        {
            "name": "目白善信",
            "text": "咻、咻～\n這樣喊對嗎？"
        },
        {
            "name": "目白善信",
            "text": "話說回來，被稱呼為好麻吉……\n果然還是有點害羞呢。"
        },
        {
            "name": "大德太陽",
            "text": "別害羞別害羞～☆"
        },
        {
            "name": "大德太陽",
            "text": "我會大秀一波的！\n好好享受吧──☆"
        },
        {
            "name": "目白善信",
            "text": "嗯，我會的！"
        },
        {
            "name": "目白善信",
            "text": "是說太陽，\n妳看起來完全都不緊張耶。"
        },
        {
            "name": "大德太陽",
            "text": "緊張？？\n為什麼？"
        },
        {
            "name": "目白善信",
            "text": "畢竟是出道戰啊……\n正常都會緊張吧？"
        },
        {
            "name": "大德太陽",
            "text": "……"
        },
        {
            "name": "大德太陽",
            "text": "不會！！"
        },
        {
            "name": "目白善信",
            "text": "什麼！？\n……好強！"
        },
        {
            "name": "大德太陽",
            "text": "因為我有超強的精神，\n應該說是超嗨的精神☆"
        },
        {
            "name": "大德太陽",
            "text": "好啦，開始吧♪"
        }
    ]
}